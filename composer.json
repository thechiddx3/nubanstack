{
    "name": "nubanstack/nubanstack",
    "description": "A PHP package for fetching Nigerian banks and validating account numbers via Paystack API",
    "type": "library",
    "keywords": ["paystack", "nigeria", "banks", "account validation", "nuban"],
    "license": "MIT",
    "authors": [
        {
            "name": "Your Name",
            "email": "your.email@example.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Nubanstack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nubanstack\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
